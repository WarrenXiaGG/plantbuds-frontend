{
    "$schema": "http://json.schemastore.org/prettierrc",
    "arrowParens": "avoid",
    "bracketSpacing": false,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "printWidth": 100,
    "proseWrap": "always",
    "quoteProps": "as-needed",
    "semi":true,
    "singleQuote": true, 
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false
  }